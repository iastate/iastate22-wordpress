        <span class="facetwp-alpha facetwp-link" data-id="F">F</span>
        <span class="facetwp-alpha facetwp-link" data-id="G">G</span>
        <span class="facetwp-alpha facetwp-link" data-id="H">H</span>
        <span class="facetwp-alpha facetwp-link" data-id="I">I</span>
        <span class="facetwp-alpha facetwp-link available" data-id="J">J</span>
