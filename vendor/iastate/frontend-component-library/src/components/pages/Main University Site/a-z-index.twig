      <li>
        {% if letter.url %}
        <a {% if letter.current_page == true  %}class="a-z-index-letter-list__active"{% endif %} href="{{ letter.url }}">{{ letter.letter }}</a>
        {% else %}
        <span>{{ letter.letter }}</span>
        {% endif %}
      </li>
      {% endfor %}
    </ul> #}
    <div class="facetwp-facet facetwp-facet-faculty_a_z facetwp-type-alpha" data-name="faculty_a_z" data-type="alpha">
        <span class="facetwp-alpha facetwp-link available" data-id="">Any</span>
