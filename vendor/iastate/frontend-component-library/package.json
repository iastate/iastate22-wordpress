{
  "name": "idfive-component-library",
  "version": "0.3.0",
  "description": "Starter component library for idfive web projects",
  "main": "index.js",
  "scripts": {
    "serve": "webpack-dev-server --mode development --open",
    "watch": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "fractal": "concurrently --kill-others-on-fail \"npm run watch\" \"fractal start --sync\"",
    "fractal:build": "fractal build",
    "fractal:component": "fractal create-component",
    "format": "prettier --write 'src/**/*.js' 'src/**/*.ts' 'src/**/*.vue' 'src/**/*.scss' 'src/**/*.json'",
    "imagemin": "cd src/img && npx @squoosh/cli --mozjpeg auto *.jpg"
  },
  "author": {
    "name": "idfive",
    "email": "code@idfive.com",
    "url": "https://idfive.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@frctl/fractal": "^1.5.15",
    "@frctl/twig": "^1.2.13",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@types/node": "^10.17.60",
    "@types/youtube-player": "^5.5.11",
    "@vimeo/player": "^2.25.0",
    "ajv": "^6.12.6",
    "autoprefixer": "^9.8.8",
    "babel-loader": "^8.4.1",
    "babel-loader-exclude-node-modules-except": "^1.2.1",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.1.1",
    "css-loader": "^2.1.1",
    "extract-loader": "^3.2.0",
    "file-loader": "^1.1.11",
    "fullcalendar": "^6.1.15",
    "html-loader": "^0.5.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.14.1",
    "normalize-scss": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.8",
    "postcss-focus-within": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-object-fit-images": "^1.1.2",
    "prettier": "^1.19.1",
    "resolve-url-loader": "^3.1.5",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "terser-webpack-plugin": "^1.4.6",
    "ts-loader": "^5.1.0",
    "typescript": "^5.7.0",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.11.1",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^4.47.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/idfivellc/idfive-component-library"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@types/bootstrap": "^5.2.10",
    "body-scroll-lock": "^3.1.5",
    "bootstrap": "^5.3.3",
    "core-js": "^3.39.0",
    "debounce": "^1.2.1",
    "flickity": "^2.3.0",
    "flickity-imagesloaded": "^2.0.0",
    "focus-within": "^3.0.2",
    "headroom.js": "^0.12.0",
    "js-cookie": "^2.2.1",
