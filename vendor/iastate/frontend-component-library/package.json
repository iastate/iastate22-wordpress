    "silc-accordion": "^0.10.2",
    "silc-core": "^0.6.0",
    "silc-grid": "^0.6.0",
    "silc-nav": "^1.0.8",
    "silc-utilities": "^0.3.2",
    "vue": "^2.7.16",
    "youtube-player": "^5.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue,scss,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ]
}
