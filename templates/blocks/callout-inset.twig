<div class="paragraph-widget paragraph-widget--callout-inset"{% if fields.anchor_target|length > 0 %} id="{{ fields.anchor_target|e('esc_attr') }}"{% endif %}>
    <div class="callout-inset">
    <div class="callout-inset_inset">
        <h2 class="callout-inset_inset-headline">{{fields.callout.title}}</h2>
        <div>{{fields.callout.content }}</div>
    </div>
    <div class="feature-with-large-image">
        <div class="feature-with-large-image__media">
            <img src="{{ fields.image.url|e('esc_url') }}" alt="{{ fields.image.alt|e('esc_attr') }}" width="{{ fields.image.width|e('esc_attr') }}" height="{{ fields.image.height|e('esc_attr') }}"/>
        </div>
        <div class="feature-with-large-image__content">
            <h2>{{ fields.inset.title }}</h2>
            <div class="feature-with-large-image__text">{{fields.inset.content}}</div>
            <div class="iastate22-button-set">
                <ul>
                    <li>
                        <a class="iastate22-button" href="{{ fields.inset.link.url|e('esc_url') }}">{{ fields.inset.link.label }}<span class="arrow"></span></a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    </div>
</div>
